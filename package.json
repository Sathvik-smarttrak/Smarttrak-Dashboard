  ],
    "win": {
      "target": "portable",
      "icon": "icon.ico"
    }
  },
  "author": "Smarttrak Ai",
  "license": "ISC",
  "devDependencies": {
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "dotenv": "^17.2.1"
  }
}
