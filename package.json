{
  "name": "smarttrakdashboard",
  "version": "1.0.2",
  "description": "Executive Dashboard app",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.smarttrak.dashboard",
    "productName": "SmartTrak Dashboard",
